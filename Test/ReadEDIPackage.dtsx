    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        string[] elements = Row.Segment.Split('*');
        int elementCount = elements.Length;
        string segmentId = elements[0];

        List<XAttribute> atts = new List<XAttribute>();
        for (int i = 1; i < elementCount; i++)
        {
            atts.Add(new XAttribute(segmentId + (i).ToString("00"), elements[i]));
        }

        XElement segment = new XElement(segmentId, atts.ToArray());
        Guid id;
        switch (segmentId)
        {
            // Es cabeza de tabla y la de mas arriba en las relaciones
            case "ISA":
                id = Guid.NewGuid();
                segment.Add(new XAttribute("Id", id));
                rootISA.Add(segment);
                parent = segment;
                
                // Tiene segmento Pie o que cierrar loop o grupos de segmentos
                currentISA = segment;
                break;

            // Es cabeza de tabla
            case "ST":
                id = Guid.NewGuid();
                segment.Add(new XAttribute("Id", id));
                segment.Add(new XAttribute("ParentId", (Guid)currentISA.Attribute("Id")));
                rootST.Add(segment);
                parent = segment;

                // Tiene segmento Pie o que cierrar loop o grupos de segmentos
                currentST = segment;
                break;

            // Es cabeza de tabla
            case "LX":
                id = Guid.NewGuid();
                segment.Add(new XAttribute("Id", id));
                segment.Add(new XAttribute("ParentId", (Guid)currentST.Attribute("Id")));
                rootLX.Add(segment);
                parent = segment;
                currentLX = segment;
                break;

            // Es cabeza de tabla
            case "CLP":
                id = Guid.NewGuid();
                segment.Add(new XAttribute("Id", id));
                segment.Add(new XAttribute("ParentId", (Guid)currentLX.Attribute("Id")));
                rootCLP.Add(segment);
                parent = segment;
                currentCLP = segment;
                break;

            // Es cabeza de tabla y l ultima en las relaciones en cascada
            case "SVC":
                id = Guid.NewGuid();
                segment.Add(new XAttribute("Id", id));
                segment.Add(new XAttribute("ParentId", (Guid)currentCLP.Attribute("Id")));
                rootSVC.Add(segment);
                parent = segment;
                break;

            // Tiene segmento Pie o que cierrar loop o grupos de segmentos 
            case "GS":
                parent.Add(segment);
                currentGS = segment;
                break;

            // Es Pie o Segmento que cierra loop o grupos de segmentos
            case "SE":
                currentST.Add(segment);
                break;

            // Es Pie o Segmento que cierra loop o grupos de 
            case "GE":
                currentGS.Add(segment);
                break;

            // Es Pie o Segmento que cierra loop o grupos de segmentos
            case "IEA":
                currentISA.Add(segment);
                break;


            // Los demas
            default:
                parent.Add(segment);

                



                        break;
        }
    }



