        string[] elements = Row.Segment.Split('*');
        string segmentId = elements[0];

        var segmentSetting = GetSegmentSetting(segmentId);
        if (segmentSetting != null)
        {
            if (segmentSetting.IsUnique)
            {
                var xmlSegment = CreateXmlSegment(elements);

                if (segmentSetting.IsTableHeader)
                {
                    var id = Guid.NewGuid();
                    xmlSegment.Add(new XAttribute("Id", id));

                    var tableHeader = (from h in TableHeaders
                                       where h.SegmentId == segmentId
                                       select h).FirstOrDefault();

                    if (tableHeader != null)
                    {
                        if (!string.IsNullOrWhiteSpace(segmentSetting.TableParent))
                        {
                            var currentParent = (from c in TableHeaders
                                                 where c.SegmentId == segmentSetting.TableParent
                                                 select c.Current
                                                 ).FirstOrDefault();

                            if (currentParent != null)
                                xmlSegment.Add(new XAttribute("ParentId", (Guid)currentParent.Attribute("Id")));
                        }
                        tableHeader.Root.Add(xmlSegment);
                        tableHeader.Current = xmlSegment;
                        Parent = xmlSegment;
                    }
                }
                else
                {
                    if (!string.IsNullOrWhiteSpace(segmentSetting.FooterOf))
                    {
                        var header = (from c in Headers
                                      where c.SegmentId == segmentSetting.FooterOf
                                      select c.Current
                                                     ).FirstOrDefault();
                        if (header != null)
                        {
                            header.Add(xmlSegment);
                        }
                    }
                    else
                    {
                        Parent.Add(xmlSegment);
                    }
                }

                if (segmentSetting.HasFooter)
                {
                    var header = (from h in Headers
                                  where h.SegmentId == segmentId
                                  select h).FirstOrDefault();

                    if (header != null)
                        header.Current = xmlSegment;
                }
            }
        }
