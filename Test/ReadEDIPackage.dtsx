    private void LoadSettings()
    {
        // Read Xml file with Segment Settings
        xmlSetting = XDocument.Load(@"C:\tmp\test\EDI835Definition.xml");
        SegmentSettings = (from item in xmlSetting.Descendants("Root").Descendants("Segments").Descendants("Segment")
             select new SegmentSetting
             {
                 SegmentId = item.Attribute("Id") != null ? item.Attribute("Id").Value : "",
                 IsUnique = item.Attribute("IsUnique") != null ? bool.Parse(item.Attribute("IsUnique").Value) : false,
                 IsTableHeader = item.Attribute("IsTableHeader") != null ? bool.Parse(item.Attribute("IsTableHeader").Value) : false,
                 TableParent = item.Attribute("TableParent") != null ? item.Attribute("TableParent").Value : "",
                 FooterOf = item.Attribute("FooterOf") != null ? item.Attribute("FooterOf").Value : "",
                 HasFooter = item.Attribute("HasFooter") != null ? bool.Parse(item.Attribute("HasFooter").Value) : false
             }).ToList();

        TableHeaders = (    from t in SegmentSettings
                            where t.IsTableHeader
                            select new TableHeader
                            {
                                SegmentId = t.SegmentId,
                                Root = new XElement("Root")
                            }).ToList();


        Headers = (from c in SegmentSettings
                   where c.HasFooter
                   select new Header
                    {
                        SegmentId = c.SegmentId
                    }).ToList();
    }

    // Return SegmentSetting fron settings
    private SegmentSetting GetSegmentSetting(string segmentId)
    {
        return (from s in SegmentSettings
                where s.SegmentId == segmentId
                select s).FirstOrDefault();
    }

